        function filterKel() {
            $('#barang_lok_kecamatan_id').change(function () {
                const selectedKecamatan = $(this).val();
                const kecId = selectedKecamatan.split("-");
                const $selectKel = $('#barang_lok_kelurahan_desa_id');
                // Hapus opsi sebelumnya
                $selectKel.empty().append('<option value="">Pilih Kelurahan</option>');
                $('#barang_lok_kelurahan_desa_id').prop('disabled', true);

                // Jika ada pilihan provinsi yang dipilih, tambahkan opsi kota
                $.ajax({
                    url: "/TanahPage/GetKelurahan",
                    type: "GET",
                    dataType: "json", // Jenis data yang diharapkan dari server
                    success: function (response) {
                        /* console.log(response); return; */
                        let kel = JSON.parse(response.data);
                        $.each(kel, function (index, kelurahan) {
                            if (kelurahan.kecamatan_id == kecId[0]) {
                                // console.log(kelurahan.nama)
                                $('#barang_lok_kelurahan_desa_id').append(`<option value="${kelurahan.kelurahan_id}_${kelurahan.nama}">${kelurahan.nama}</option>`);
                            }
                        });
                    },
                    error: function (xhr, status, error) {
                        console.error("Terjadi kesalahan: " + error); // Error handling
                    }
                });
                $('#barang_lok_kelurahan_desa_id').prop('disabled', false);
            });
        }

